<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core" level="3" version="1">

  <model substanceUnits="mole" timeUnits="second" extentUnits="mole">

    <listOfUnitDefinitions>
      <unitDefinition id="per_second">
        <listOfUnits>
          <unit kind="second" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>

    <listOfCompartments>
      <compartment id="c1" spatialDimensions="3" size="1" units="litre" constant="true"/>
      <compartment id="c2" spatialDimensions="3" size="1" units="litre" constant="true"/>
      <compartment id="c3" spatialDimensions="3" size="1" units="litre" constant="true"/>
    </listOfCompartments>

    <listOfSpecies>
      <species id="s1" compartment="c1" initialAmount="5" initialConcentration="1" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
            
      <species id="s2" compartment="c2" initialAmount="5" initialConcentration="1" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      
      <species id="s3" compartment="c1" initialAmount="5" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      
      <species id="s4" compartment="c1" initialAmount="5" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>


    </listOfSpecies>

    <listOfParameters>
      <parameter id="k" value="1267" units="per_second" constant="true"/>
    </listOfParameters>

    <listOfReactions>

      <reaction id="r1" reversible="false" fast="false"  compartment="c1">
        <listOfReactants>
          <speciesReference species="s1" constant="true"/>
        </listOfReactants>
      </reaction>

      <reaction id="r2" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference species="s1" constant="true"/>
          <speciesReference species="s2" constant="true"/>
        </listOfReactants>
      </reaction>

      <reaction id="r3" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference species="s1" constant="true"/>
          <speciesReference species="s2" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="s3" constant="true"/>
        </listOfProducts>
      </reaction>

      <reaction id="r4" reversible="false" fast="false">
        <listOfProducts>
          <speciesReference species="s1" constant="true"/>
        </listOfProducts>
      </reaction>

    </listOfReactions>
  </model>
</sbml>
